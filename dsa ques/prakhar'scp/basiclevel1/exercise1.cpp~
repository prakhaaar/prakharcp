#include <iostream>
using namespace std;

int main() {
    int input_marks;
    cout << "Enter marks (0â€“100): ";
    cin >> input_marks;

    if (input_marks >= 90 && input_marks <= 100) {
        cout << "Grade: A+" << endl;
    } 
    else if (input_marks >= 80) {
        cout << "Grade: A" << endl;
    } 
    else if (input_marks >= 70) {
        cout << "Grade: B" << endl;
    } 
    else if (input_marks >= 60) {
        cout << "Grade: C" << endl;
    } 
    else if (input_marks >= 50) {
        cout << "Grade: D" << endl;
    } 
    else if (input_marks >= 0) {
        cout << "Grade: F" << endl;
    } 
    else {
        cout << "Invalid Marks!" << endl;
    }

    return 0;
}

